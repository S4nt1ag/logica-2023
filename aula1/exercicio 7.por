programa 
{
  funcao inicio() 
  {
     inteiro d1, d2, r1, q1

     escreva("digite oque deseja dividir:")
      leia(d1)

     escreva("digite por quanto deseja divitir:")
      leia(d2)

     q1 = d1/ d2
     r1 = d1% d2


     escreva("Dividendo:", d1, "\n", "Divisor:", d2, "\n", "Quociente:", q1, "\n", "Resto:", r1)     
  }
}
